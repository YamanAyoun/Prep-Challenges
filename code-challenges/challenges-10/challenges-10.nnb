                "function birthdayCakeCandles(candles) {\n    // Write your code here\n    let max = 0;\n    let count = 0;\n    \n    for (let i = 0; i < candles.length; i++) {\n        if (candles[i] > max) {\n            max = candles[i];\n            count = 1;\n        } else if (candles[i] === max) {\n            count++;\n        }\n    }\n    \n    return count;\n\n}"
